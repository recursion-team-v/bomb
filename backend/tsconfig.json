{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es6", // ESNext などにすると動かない
    "module": "commonjs",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["/"]
}
